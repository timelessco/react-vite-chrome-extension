{
    "name": "react-vite-chrome-extension",
    "description": "Starter pack for Chrome extension development uses React + Vite",
    "version": "0.0.1",
    "manifest_version": 3,
    "icons": {
        "16": "logo16.png",
        "48": "logo48.png",
        "128": "logo128.png"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "logo16.png",
            "48": "logo48.png",
            "128": "logo128.png"
        }
    },
    "permissions": [
        "storage",
        "tabs"
    ],
    "homepage_url": "https://timeless.co"
}